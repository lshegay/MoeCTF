{
  "name": "moectf-core",
  "version": "0.0.1",
  "description": "MoeCTF Core",
  "repository": {
    "url": "https://github.com/Noramire/MoeCTF"
  },
  "author": "noramire",
  "license": "MIT",
  "scripts": {
    "build": "node build.js",
    "test": "jest",
    "dev": "nodemon --watch \"src/**\" --ext \"js,jsx,ts,tsx,json\" --exec \"node build.js --dev\"",
    "start": "NODE_ENV=production node .build/server.js"
  },
  "exports": {
    "./response": "./.build/utils/response.js",
    "./request": "./.build/utils/request.js",
    "./models": "./.build/models/index.js"
  },
  "typesVersions": {
    "*": {
      "*": [
        "./.build/app.d.ts"
      ],
      "response": [
        "./.build/utils/response.d.ts"
      ],
      "request": [
        "./.build/utils/request.d.ts"
      ],
      "models": [
        "./.build/models/index.d.ts"
      ]
    }
  },
  "dependencies": {
    "@babel/runtime": "^7.17.2",
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "express": "^4.17.3",
    "express-fileupload": "^1.3.1",
    "express-session": "^1.17.2",
    "express-zod-api": "^7.3.1",
    "fs-extra": "^10.0.0",
    "lodash": "^4.17.21",
    "moment": "^2.29.1",
    "nedb-promises": "^5.0.3",
    "nedb-promises-session-store": "^1.0.4",
    "passport": "^0.5.2",
    "zod": "^3.17.3"
  },
  "devDependencies": {
    "@faker-js/faker": "^6.0.0-alpha.5",
    "@swc/cli": "^0.1.55",
    "@swc/core": "^1.2.141",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.2",
    "@types/express-fileupload": "^1.1.2",
    "@types/express-session": "^1.17.0",
    "@types/fs-extra": "^8.1.0",
    "@types/jest": "^27.4.0",
    "@types/lodash": "^4.14.178",
    "@types/node": "^17.0.8",
    "@types/node-fetch": "^2.6.1",
    "@types/passport": "^1.0.7",
    "@typescript-eslint/eslint-plugin": "^5.12.0",
    "@typescript-eslint/parser": "^5.12.0",
    "eslint": "^8.9.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.25.4",
    "jest": "^27.4.5",
    "node-fetch": "^2.6.6",
    "nodemon": "^2.0.15",
    "prettier": "2.6.2",
    "ts-jest": "^27.1.2",
    "typescript": "^4.5.5",
    "url-join": "^4.0.1"
  }
