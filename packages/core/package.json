{
  "name": "moectf-core",
  "version": "0.0.1",
  "description": "MoeCTF Core",
  "repository": {
    "url": "https://github.com/Noramire/MoeCTF"
  },
  "author": "noramire",
  "license": "MIT",
  "scripts": {
    "build": "node build.js",
    "test": "jest",
    "dev": "nodemon --watch \"src/**\" --ext \"js,jsx,ts,tsx,json\" --exec \"node build.js --dev\"",
    "start": "NODE_ENV=production node .build/server.js"
  },
  "exports": {
    "./response": "./.build/utils/response.js",
    "./request": "./.build/utils/request.js",
    "./models": "./.build/models/index.js"
  },
  "typesVersions": {
    "*": {
      "*": ["./.build/app.d.ts"],
      "response": ["./.build/utils/response.d.ts"],
      "request": ["./.build/utils/request.d.ts"],
      "models": ["./.build/models/index.d.ts"]
    }
  },
  "dependencies": {
    "@babel/runtime": "^7.16.7",
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "eslint-import-resolver-typescript": "^2.5.0",
    "express": "^4.17.2",
    "express-fileupload": "^1.2.1",
    "express-session": "^1.17.1",
    "fs-extra": "^8.1.0",
    "lodash": "^4.17.15",
    "nedb-promises": "^5.0.0",
    "nedb-promises-session-store": "^1.0.4",
    "passport": "^0.4.1"
  },
  "devDependencies": {
    "@swc/cli": "^0.1.55",
    "@swc/core": "^1.2.135",
    "@types/express": "^4.17.2",
    "@types/express-fileupload": "^1.1.2",
    "@types/express-session": "^1.17.0",
    "@types/fs-extra": "^8.1.0",
    "@types/jest": "^27.4.0",
    "@types/lodash": "^4.14.149",
    "@types/node": "^13.7.6",
    "@types/node-fetch": "^2.5.12",
    "@types/passport": "^1.0.2",
    "@typescript-eslint/eslint-plugin": "^5.8.1",
    "@typescript-eslint/parser": "^5.8.1",
    "eslint": "^8.2.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.25.2",
    "jest": "^27.4.5",
    "node-fetch": "^2.6.6",
    "nodemon": "^2.0.12",
    "ts-jest": "^27.1.2",
    "typescript": "^4.5.4"
  }
}
