{
  "compilerOptions": {
    "jsx": "preserve",
    "esModuleInterop": true,
    "target": "esnext",
    "lib": [
      "es2015",
      "es2016",
      "es2017",
      "dom"
    ],
    "skipLibCheck": true,
    "strict": false,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "removeComments": false,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "allowJs": true,
    "isolatedModules": true
  },
  "exclude": [
    "node_modules",
    "server"
  ],
  "include": [
    "**/*.ts",
    "**/*.tsx"
  ]
}
