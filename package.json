{
  "name": "moectf-core",
  "version": "0.1.0",
  "description": "MoeCTF embedded server base built on top Express",
  "repository": {
    "url": "https://github.com/Noramire/MoeCTF/tree/base"
  },
  "main": ".build/app.js",
  "author": "noramire",
  "license": "MIT",
  "scripts": {
    "build": "tsc --project tsconfig.server.json && next build",
    "restore": "ts-node --project tsconfig.server.json test/restore.ts",
    "test": "jest",
    "dev": "nodemon --watch \"src/**\" --ext \"js,jsx,ts,tsx,json,\" --exec \"ts-node -P tsconfig.server.json src/server.ts\"",
    "start": "NODE_ENV=production node .build/server.js"
  },
  "dependencies": {
    "@geist-ui/react": "^2.2.0",
    "body-parser": "^1.19.0",
    "cross-env": "^7.0.3",
    "express": "^4.17.1",
    "express-fileupload": "^1.2.1",
    "express-session": "^1.17.1",
    "fs-extra": "^8.1.0",
    "inter-ui": "^3.19.3",
    "lodash": "^4.17.15",
    "nedb-promises": "^5.0.0",
    "next": "^11.1.2",
    "passport": "^0.4.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "swr": "^1.0.1",
    "typescript": "^4.2.3"
  },
  "devDependencies": {
    "@types/express": "^4.17.2",
    "@types/express-fileupload": "^1.1.2",
    "@types/express-session": "^1.17.0",
    "@types/fs-extra": "^8.1.0",
    "@types/lodash": "^4.14.149",
    "@types/node": "^13.7.6",
    "@types/passport": "^1.0.2",
    "@types/react": "^17.0.20",
    "@typescript-eslint/eslint-plugin": "^2.21.0",
    "@typescript-eslint/parser": "^2.21.0",
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.25.1",
    "eslint-plugin-react-hooks": "^4.2.0",
    "jest": "^27.2.0",
    "nodemon": "^2.0.12",
    "sass": "^1.40.1",
    "ts-node": "^9.1.1"
  }
}
